
{
    "manifest_version" : 3,
    "version" : "1.0.0",
    "name" : "Allerfree",
    "icons": {
        "32" : "gwc-logo-circle.png"
    },

    "action": {
        "default_popup": "index.html"
    },
    "permissions": ["scripting", "tabs", "activeTab"],
    "background":{
        "service_worker": "service-worker.js",
        "type": "module"
    },
    "host_permissions": [
        "http://*/*",
        "https://*/*"
    ],
    "content_scripts": [
    {
      "matches": ["https://www.google.com/maps/*"],
      "js": ["content-script.js"],
      "run_at": "document_idle",
      "all_frames": false
    }
  ]
}
  


  
    
  